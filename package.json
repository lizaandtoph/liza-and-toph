    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "esbuild": "^0.25.0",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "vite": "^5.4.20"
  }
