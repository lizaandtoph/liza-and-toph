    "seed:verify-prod-block": "NODE_ENV=production tsx server/storage/seed.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@neondatabase/serverless": "^0.10.4",
    "bcryptjs": "^3.0.2",
